{
  "name": "id.central.connect",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cross-env NODE_ENV=development react-native run-android",
    "android-release": "cd android && ./gradlew clean && cross-env NODE_ENV=production react-native run-android --mode=release",
    "android-release-build": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "android-release-bundle": "cd android && ./gradlew clean && cross-env NODE_ENV=production ./gradlew bundleRelease && cd ..",
    "android-clean": "cd android && ./gradlew clean && cd .. && react-native run-android ",
    "android-codepush": "appcenter codepush release-react -a Central-Connect-App/Android -d Production",
    "android-codepush-staging": "appcenter codepush release-react -a Central-Connect-App/Android -d Staging",
    "ios": "cross-env NODE_ENV=development react-native run-ios --simulator=\"iPhone 15\"",
    "ios-clean-full": "rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && cd ios && xcodebuild clean && pod cache clean --all && pod deintegrate && pod setup && pod install --verbose && cd ..",
    "ios-codepush": "appcenter codepush release-react -a Central-Connect-App/IOS -d Production",
    "ios-codepush-staging": "appcenter codepush release-react -a Central-Connect-App/IOS -d Staging",
    "pod": "cd ios && NO_FLIPPER=1 pod install && cd ..",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "format-check-style": "prettier --check ./src",
    "format-fix-style": "npx prettier --write ./src",
    "depcheck": "npx depcheck "
  },
